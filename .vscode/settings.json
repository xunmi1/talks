{
  "files.eol": "\n",
  "editor.tabSize": 2,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.arrowParens": "avoid",
  "prettier.printWidth": 120,
  "prettier.tabWidth": 2,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "prettier.semi": true,
  "prettier.endOfLine": "lf",
  "cSpell.words": [
    "slidev",
    "pnpm",
    "vite",
  ],
  "cSpell.allowCompoundWords": true,
}
